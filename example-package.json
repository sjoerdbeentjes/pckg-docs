{
  "name": "eleventy-datocms",
  "version": "0.0.1",
  "description": "Test Eleventy with Dato CMS",
  "scripts": {
    "start": "npm run dev",
    "dev": "run-p dev:clean dev:watch dev:webpack dev:serve",
    "dev:clean": "rimraf dist && rimraf build",
    "dev:watch": "nodemon --exec npm run dev:build",
    "dev:build": "node scripts/transpile.js && cross-env ELEVENTY_ENV=dev eleventy --quiet",
    "dev:webpack": "cross-env NODE_ENV=dev webpack --watch",
    "dev:serve": "browser-sync start --ss dist --watch --files 'dist' --ignore 'dist/js' --no-notify --reload-debounce 10",
    "build": "run-s build:*",
    "build:svelte": "node scripts/transpile",
    "build:static": "cross-env ELEVENTY_ENV=prod eleventy",
    "build:client": "cross-env NODE_ENV=prod webpack",
    "seed": "cross-env ELEVENTY_ENV=seed eleventy"
  },
  "author": "Sjoerd Beentjes",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-fetch": "^2.6.0",
    "webpack": "^4.41.6"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "browser-sync": "^2.26.7",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "eleventy-plugin-sass": "^1.0.0",
    "lodash.debounce": "^4.0.8",
    "mkdirp": "^1.0.3",
    "moment": "^2.24.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^1.31.1",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.1.1",
    "sass-loader": "^8.0.2",
    "serve": "^11.3.0",
    "style-loader": "^1.1.3",
    "svelte": "^3.18.2",
    "svelte-loader": "^2.13.6",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-manifest-plugin": "^2.2.0"
  }
}
